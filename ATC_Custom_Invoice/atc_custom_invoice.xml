<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width: 100%;"><tr>
	<td rowspan="3"><#if companyInformation.logoUrl?length != 0><img height="40%" src="${companyInformation.logoUrl}" style="float: left; margin: 7px" width="40%" /> </#if> ${subsidiary.name}&nbsp;&nbsp;<br />${subsidiary.mainaddress_text}<br /><br />${subsidiary.email}<br />&nbsp;</td>
	<td align="right"><span class="title">${record@title}</span></td>
	</tr></table>
        </macro>
        <macro id="nlfooter">
            <table class="footer" style="width: 100%;"><tr>
	<td><barcode codetype="code128" showtext="true" value="${record.tranid}"/></td>
	<td align="right"><pagenumber/> of <totalpages/></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">* {
		<#if .locale == "zh_CN">
			font-family: NotoSans, NotoSansCJKsc, sans-serif;
		<#elseif .locale == "zh_TW">
			font-family: NotoSans, NotoSansCJKtc, sans-serif;
		<#elseif .locale == "ja_JP">
			font-family: NotoSans, NotoSansCJKjp, sans-serif;
		<#elseif .locale == "ko_KR">
			font-family: NotoSans, NotoSansCJKkr, sans-serif;
		<#elseif .locale == "th_TH">
			font-family: NotoSans, NotoSansThai, sans-serif;
		<#else>
			font-family: NotoSans, sans-serif;
		</#if>
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
        th {
            font-weight: bold;
            font-size: 7pt;
            vertical-align: middle;
            padding: 4px 5px 2px;
            background-color: #e3e3e3;
            color: #333333;
        }
        td {
            padding: 4px 6px;
        }
		td p { align:left }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0px;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0px;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 8px;
            padding-top: 8px;
        }
        table.body td {
            padding-top: 2px;
        }
        table.total {
            page-break-inside: avoid;
        }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 200%;
        }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 8pt;
            padding-top: 6px;
            padding-bottom: 2px;
        }
        td.address {
            padding-top: 0px;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
        }
        td.totalboxbot {
            font-weight: bold;
        }
        span.title {
            font-size: 20pt;
            text-transform: uppercase;
            font-weight: bold;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="10%" footer="nlfooter" footer-height="20pt" padding="0.4in 0.4in 0.4in 0.4in" size="Letter">
    <table style="width: 100%; margin-top: 10px;"><tr>
	<td class="addressheader" colspan="7"><b>${record.billaddress@label}</b></td>
	<td class="addressheader" colspan="7"><b>${record.shipaddress@label}</b></td>
	<td class="addressheader" colspan="7">&nbsp;</td>
	<td class="addressheader" colspan="9"><b><span style="font-size:12px;">Invoice#:&nbsp;${record.tranid}</span></b><br />&nbsp;</td>
	</tr>
	<tr>
	<td class="address" colspan="7" rowspan="2">${record.billaddress}<br /></td>
	<td class="address" colspan="7" rowspan="2">${record.shipaddress}</td>
	<td class="address" colspan="7" rowspan="2">&nbsp;</td>
	<td class="address" colspan="7" rowspan="2"><b>Invoice Date:&nbsp;${record.trandate}</b><br /><br /><b>Payment Terms:&nbsp;${record.terms}</b><br /><br /><b>Due Date:&nbsp;${record.duedate}</b><br /><br /><b>PO #:&nbsp;${record.otherrefnum}</b><br />&nbsp;</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	</tr></table>
<#if record.item?has_content>

<table class="itemtable" style="width: 100%; margin-top: 20px;"><!-- start items --><#list record.item as item><#if item_index==0>
<thead>
	<tr>
	<th style="align:left" colspan="5">Facility</th>
    <th style="align:left" colspan="7">${item.item@label}</th>
	<th colspan="4">
	<p style="align:center">Service Start Date</p>
	</th>
	<th colspan="4">
	<p style="align:center">Service End Date</p>
	</th>
	<th align="center" colspan="3">Qty</th>
	<th align="center" colspan="4">${item.rate@label}</th>
	<th align="center" colspan="4">${item.amount@label}</th>
	</tr>
</thead>
</#if><tr>
	<!-- Strip parent hierarchy from facility segment -->
	<#if item.cseg3?contains(":")>
	<td align="left" colspan="5" line-height="150%">${item.cseg3?keep_after_last(":")}</td>
	<#else>
	<td align="left" colspan="5" line-height="150%">${item.cseg3}</td>
	</#if>
    <td align="left" colspan="7"><span class="itemname">${item.item}</span><br />${item.description}</td>
	<td align="center" colspan="4" line-height="150%">${item.custcol_mfc_service_start_date}</td>
	<td align="center" colspan="4" line-height="150%">${item.custcol_mfc_service_end_date}</td>
	<td align="center" colspan="3" line-height="150%">${item.quantity}</td>
	<td align="center" colspan="4">${item.rate}</td>
	<td align="center" colspan="4">${item.amount}</td>
	</tr>
	</#list><!-- end items --></table>

<hr /></#if>
<table class="total" style="width: 100%; margin-top: 10px;"><tr>
	<td colspan="4">&nbsp;</td>
	<td align="right"><b>${record.subtotal@label}</b></td>
	<td align="right">${record.subtotal}</td>
	</tr>
	<tr>
	<td align="left" colspan="4">MEMO: ${record.memo}</td>
	</tr>
	<tr>
	<td colspan="4">&nbsp;</td>
	<td align="right"><b>${record.taxtotal@label} (${record.taxrate}%)</b></td>
	<td align="right">${record.taxtotal}</td>
	</tr>
	<tr class="totalrow">
	<td background-color="#ffffff" colspan="4">&nbsp;</td>
	<td align="right"><b>${record.total@label}</b></td>
	<td align="right">${record.total}</td>
	</tr></table>

<table style="padding-left:10px"><tr>
	<td>&nbsp;</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	</tr>
	<tr>
	<td colspan="5"><b>*Please note effective January 1, 2023, the legal entity name and payment information for your invoice may have changed. Please review all attachments included with your invoice.</b></td>
	</tr>
	<tr>
	<td colspan="5">**All invoices past due may be subject to late fees</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	</tr>
	<tr>
	<td colspan="3">Please make check payable to:&nbsp;${record.subsidiary}</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	</tr></table>

<table align="left" style="width:750px;"><tr>
	<td><b>Remit Payment Address:</b><br />${subsidiary.custrecord_atc_bank_name}<br />${subsidiary.custrecord_act_lockbox}<br />${subsidiary.custrecord153}</td>
	<td><b>Overnight Remit Address:</b><br />${subsidiary.custrecord_atc_bank_name}<br />${subsidiary.custrecord_act_lockbox}<br />${subsidiary.custrecord152}<br />&nbsp;</td>
	<td><b>ACH Payment Instructions: </b><br />${subsidiary.custrecord_atc_bank_name}<br />${subsidiary.custrecord_atc_act_name}<br />Account Number: ${subsidiary.custrecord_atc_bank_acct_num}<br />Routing Number: ${subsidiary.custrecord_act_rout_num}</td>
	</tr></table>
<br />&nbsp;
</body>
</pdf>